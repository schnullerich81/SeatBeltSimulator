                <styles:SHButtonPrimary x:Name="SelectPort" HorizontalAlignment="Left" Click="SHButtonPrimary_Click_1">Anschluss wählen</styles:SHButtonPrimary>
                <styles:SHButtonSecondary x:Name="ButtonIn" HorizontalAlignment="Left" Click="SHButtonPrimary_Click">Einfahren</styles:SHButtonSecondary>
                <styles:SHButtonSecondary HorizontalAlignment="Left" Click="SHButtonSecondary_Click">Ausfahren</styles:SHButtonSecondary>
                <!--styles:SHLinkButton HorizontalAlignment="Left">Link button</>
                <styles:SHLinkButtonSmall HorizontalAlignment="Left">Link button small</styles:SHLinkButtonSmall-->
                <!--ui:ControlsEditor FriendlyName="Increment Speed Warning" ActionName="DataPluginDemo.IncrementSpeedWarning"></-->  
            </StackPanel>
        </styles:SHSection>
    </Grid>
